{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "complexity": ["error", { "max": 20 }],
        "max-len": ["error", { "code": 190 }]
    }
}
