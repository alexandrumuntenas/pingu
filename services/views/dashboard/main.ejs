    <script>
        function lightMeUp() {
            if ($('body').hasClass('theme-dark')) {
                $('body').removeClass('theme-dark');
            } else {
                $('body').addClass('theme-dark');
            }
        };
        function getMeOut() {
            $.ajax({
                type: "POST",
                url: '/logout',
                beforeSend: function () {
                    Toast.fire({
                        icon: 'warning',
                        title: 'Cerrando sesi√≥n...'
                    });
                },
                success: function (data) {
                    Toast.fire({
                        icon: 'success',
                        title: 'Redireccionando...'
                    });
                    setTimeout(function () {
                        location.reload();
                    }, 3000);
                }
            });
        }
