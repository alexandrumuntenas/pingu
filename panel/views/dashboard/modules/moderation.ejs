    <script>
        const Toast = Swal.mixin({
            toast: true,
            position: 'bottom',
            showConfirmButton: false,
            timer: 3000,
            timerProgressBar: true
        })
        $("#Y2adeog").change(function () {
            $('#DJxuG8K').removeAttr('disabled');
        });
        $("#OxV1juz").change(function () {
            $('#DJxuG8K').removeAttr('disabled');
        });
        $("#DHLJ2YI").change(function () {
            $('#DJxuG8K').removeAttr('disabled');
