                                style="background-image: url(<%= guild.iconURL({ dynamic: true, format: 'jpg' }) || 'https://cdn.discordapp.com/embed/avatars/0.png'%>)"></span>
                            <div class="d-none d-xl-block ps-2">
                                <div>
                                    <%= guild.name %>
                                </div>
                            </div>
                        </a>
                        <div class="dropdown-menu dropdown-menu-end dropdown-menu-arrow">
                            <button onclick="lightMeUp()" class="dropdown-item">
                                <%= i18n.nav.lightMeUp %>
                            </button>
                            <a href="#" class="dropdown-item">
                                <%= i18n.nav.getMeOut %>
                            </a>
