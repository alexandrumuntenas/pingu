                                                    transform="rotate(15 12 12) translate(0 -1)"></path>
                                                <line x1="3" y1="21" x2="21" y2="21"></line>
                                            </svg>
                                            <h3 class="card-title">
                                                <%= i18n.main.settings.welcome.cardTitle %>
                                            </h3>
                                        </div>
                                        <div class="card-body" style="position: relative;">
                                            <div class="form-group mb-3">
                                                <div class="form-check form-switch">
                                                    <% if(bbdd.welcome_enabled===1 ) {%>
                                                        <input type="checkbox" checked="checked" id="LNV5Ljl"
                                                            name="LNV5Ljl" class="form-check-input" value="LNV5Ljl" />
                                                        <% } else { %>
                                                            <input type="checkbox" class="form-check-input" id="LNV5Ljl"
                                                                name="LNV5Ljl" value="LNV5Ljl" />
                                                            <% } %>
                                                                <label class="form-check-label" for="LNV5Ljl">
                                                                    <%= i18n.main.settings.welcome.cardBody.LNV5Ljl.formLabel
                                                                        %>
                                                                </label>
                                                </div>
                                            </div>
                                            <div class="form-group mb-3">
                                                <label class="form-label" for="AZGW50Tc4p">
                                                    <%= i18n.main.settings.welcome.cardBody.AZGW50Tc4p.formLabel %>
                                                </label>
                                                <textarea name="AZGW50Tc4p" id="AZGW50Tc4p" class="form-control"
                                                    required><%= bbdd.welcome_message %></textarea>
                                            </div>
                                            <div class="form-group mb-3">
                                                <label class="form-label" for="daLuxtTuG5">
                                                    <%= i18n.main.settings.welcome.cardBody.daLuxtTuG5.formLabel %>
                                                </label>
                                                <select class="form-select" name="daLuxtTuG5" id="daLuxtTuG5" required>
                                                    <% channels.forEach(c=> {
                                                        if(c.channel_id == bbdd.welcome_channel){
                                                        %>
                                                        <option value="<%= c.channel_id %>" selected>#<%= c.channel_name
                                                                %>
                                                        </option>
                                                        <% } else { %>
                                                            <option value="<%= c.channel_id %>">#<%= c.channel_name %>
                                                            </option>
                                                            <% }}) %>
                                                </select>
                                            </div>
                                            <div class="form-group mb-3">
                                                <div class="form-check form-switch">
                                                    <% if(bbdd.welcome_image===1 ) {%>
                                                        <input type="checkbox" checked="checked"
                                                            class="form-check-input" id="vyKS7bC" name="vyKS7bC"
                                                            value="vyKS7bC" />
                                                        <% } else { %>
                                                            <input type="checkbox" class="form-check-input" id="vyKS7bC"
                                                                name="vyKS7bC" value="vyKS7bC" />
                                                            <% } %>
                                                                <label class="form-check-label" for="vyKS7bC">
