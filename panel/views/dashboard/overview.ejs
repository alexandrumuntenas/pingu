                                                <div class="form-group">
                                                    <select name="nviCCd9jDc" id="nviCCd9jDc" class="form-select"
                                                        multiple="multiple"
                                                        data-placeholder="Escriba para buscar roles...">
                                                        <% roles.forEach(r=> {
                                                            if(savedRoles.has(r.role_id)){
                                                            %>
                                                            <option value="<%= r.role_id %>" selected>@<%= r.role_name
                                                                    %>
                                                            </option>
                                                            <% } else { %>
                                                                <option value="<%= r.role_id %>">@<%= r.role_name %>
                                                                </option>
                                                                <% }}) %>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="card-footer">
                                            <button class="btn btn-primary" id="qYgCExvNIy" type="submit" disabled>
                                                <%= i18n.main.save %>
                                            </button>
                                        </div>
                                    </form>
